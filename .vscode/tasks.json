{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: g++.exe build active file",
            "command": "g++",
            "args": [
                "-fdiagnostics-color=always",
                "-g",
                "${workspaceFolder}/src/main.cpp",
                "${workspaceFolder}/test/DustbinTests.cpp",
                "${workspaceFolder}/src/dustbin/*.cpp",
                "${workspaceFolder}/src/exceptions/*.cpp",
                "${workspaceFolder}/src/garbage/*.cpp",
                "-I${workspaceFolder}/include/dustbin/",
                "-I${workspaceFolder}/include/exceptions/",
                "-I${workspaceFolder}/include/garbage/",
                "-I${workspaceFolder}/test/",
                  "-o",
                "${workspaceFolder}/output/${fileBasenameNoExtension}"
            ],
            "options": {
                "cwd": "${workspaceFolder}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        }
    ],
    "version": "2.0.0"
}